{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "isolatedModules": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "spot-checkin-app",
    "spot-checkin-app/**",
    "scripts",
    "scripts/**",
    "functions",
    "functions/**"
  ]
}
