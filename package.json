    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@sentry/react-native": "^7.11.0",
    "expo": "~54.0.30",
    "expo-asset": "^12.0.12",
    "expo-blur": "^15.0.8",
    "expo-camera": "~17.0.10",
